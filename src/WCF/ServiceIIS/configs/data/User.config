<?xml version="1.0" encoding="UTF-8"?>

<CmdList xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
  <CmdLine Cmdtype="SqlText" Name="getaccount" UseDataBase="NTSData">
    <Cmdtext>
      <![CDATA[ 
     select username,userpass from Becm_SystemUser where username=@username and status=0
    ]]>
    </Cmdtext>
    <param name="@username" dbType="NVarChar" size="100"/>
  </CmdLine>
</CmdList>
